ifneq (,$(4.6.1.cubemaps_skybox_config))
	@echo "==== Building 4.6.1.cubemaps_skybox ($(4.6.1.cubemaps_skybox_config)) ===="
	@${MAKE} --no-print-directory -C . -f 4.6.1.cubemaps_skybox.make config=$(4.6.1.cubemaps_skybox_config)
endif

4.6.2.cubemaps_environment_mapping:
ifneq (,$(4.6.2.cubemaps_environment_mapping_config))
	@echo "==== Building 4.6.2.cubemaps_environment_mapping ($(4.6.2.cubemaps_environment_mapping_config)) ===="
	@${MAKE} --no-print-directory -C . -f 4.6.2.cubemaps_environment_mapping.make config=$(4.6.2.cubemaps_environment_mapping_config)
endif

